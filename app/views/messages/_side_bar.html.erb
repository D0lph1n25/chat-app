<div class="side-bar_top">
  <div class="side_name">
    <%= link_to current_user.name, edit_user_path(current_user)%>
  </div>
  <div class="side_create">
    <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>

<div class="side-bar_under">
  <% current_user.rooms.each do |room| %>
    <div class="side_room">
      <div class="side_room_name">
        <a href="#"><%= room.name%></a>
      </div>
    </div>
  <% end%>
</div>